{
  "name": "ninja",
  "version": "1.0.0",
  "description": "Site do ninja no Heroku",
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "client": "cd client/ && npm start",
    "server": "npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "express": "^4.14.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cirino/react.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "author": "Dagmar Cirino",
  "contributors": [
    "Dagmar Cirino"
  ],
  "license": "MIT"
}
